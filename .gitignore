# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
.idea/workspace.xml
.idea/vcs.xml
.idea/scopes/scope_settings.xml
.idea/project.iml
.idea/modules.xml
.idea/misc.xml
.idea/encodings.xml
.idea/.name
